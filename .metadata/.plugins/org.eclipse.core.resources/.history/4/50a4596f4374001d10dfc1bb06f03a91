class Member {

	int id;
	String pw;
	String name;
	String email;
	public Member(int id, String pw, String name, String email) {
		this.id = id;
		this.pw = pw;
		this.name = name;
		this.email = email;
	}
	@Override
	public String toString() {
		return "Member [id=" + id + ", pw=" + pw + ", name=" + name + ", email=" + email + ", getClass()=" + getClass()
				+ ", hashCode()=" + hashCode() + ", toString()=" + super.toString() + "]";
	}	
}

class Board {
	
	int bno;
	String title;
	String writher;
	String content;
	public Board(int bno, String title, String writher, String content) {
		this.bno = bno;
		this.title = title;
		this.writher = writher;
		this.content = content;
	}
	@Override
	public String toString() {
		return "Board [bno=" + bno + ", title=" + title + ", writher=" + writher + ", content=" + content + "]";
	}
	
}